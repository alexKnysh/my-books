{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts-ts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "start": "react-scripts-ts start",
    "clean": "rimraf build",
    "start:dev": "npm-run-all clean -p watch-css start",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0-beta.10",
    "primereact": "^2.0.0-beta.9",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "23.3.5",
    "@types/node": "10.11.7",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "@types/react-router-config": "^1.0.9",
    "@types/react-router-dom": "^4.3.1",
    "typescript": "3.1.3"
  }
}
